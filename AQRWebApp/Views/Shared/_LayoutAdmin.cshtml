<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - WMS</title>

    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" href="@Url.Content("~/Content/AdminLTE230/bootstrap/css/bootstrap.min.css")">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="@Url.Content("~/Content/AdminLTE230/plugins/jvectormap/jquery-jvectormap-1.2.2.css")">

    
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/Content/AdminLTE230/dist/css/AdminLTE.min.css")">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="@Url.Content("~/Content/AdminLTE230/dist/css/skins/_all-skins.min.css")">

    <link rel="stylesheet" href="@Url.Content("~/Content/Site.css")">
    
    <script src="@Url.Content("~/Scripts/jQuery-1.11.3.min.js")"></script>
    <!-- jQuery 2.1.4 -->
    <script src="@Url.Content("~/Content/AdminLTE230/plugins/jQuery/jQuery-2.1.4.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-migrate-1.2.1.min.js")"></script>
    

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body class="hold-transition skin-blue sidebar-mini">

    <div class="wrapper">
        
        @Html.Partial("_PartialAdminTopNavigation")
        
        <!-- Left side column. contains the logo and sidebar -->
        @Html.Partial("_PartialAdminLeftNavigation")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>

        @Html.Partial("_PartialAdminFooter")
        
        @Html.Partial("_PartialAdminControlSidebar")
        
    </div>

    
    <!-- Bootstrap 3.3.5 -->
    <script src="@Url.Content("~/Content/AdminLTE230/bootstrap/js/bootstrap.min.js")"></script>
    
    <!-- FastClick -->
    <script src="@Url.Content("~/Content/AdminLTE230/plugins/fastclick/fastclick.min.js")"></script>
    <!-- AdminLTE App -->
    
    <!-- Sparkline -->
    <script src="@Url.Content("~/Content/AdminLTE230/plugins/sparkline/jquery.sparkline.min.js")"></script>
    <!-- jvectormap -->
    <script src="@Url.Content("~/Content/AdminLTE230/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js")"></script>
    <script src="@Url.Content("~/Content/AdminLTE230/plugins/jvectormap/jquery-jvectormap-world-mill-en.js")"></script>
    <!-- SlimScroll 1.3.0 -->
    <script src="@Url.Content("~/Content/AdminLTE230/plugins/slimScroll/jquery.slimscroll.min.js")"></script>
    <!-- ChartJS 1.0.1 -->
    <script src="@Url.Content("~/Content/AdminLTE230/plugins/chartjs/Chart.min.js")"></script>
    <script src="@Url.Content("~/Scripts/moment.js")"></script>


    <script src="@Url.Content("~/Content/AdminLTE230/dist/js/app.min.js")"></script>
    
    <!-- AdminLTE for demo purposes -->
    <script src="@Url.Content("~/Content/AdminLTE230/dist/js/demo.js")"></script>    

    <script src="@Url.Content("~/Scripts/jqueryApp.js")"></script>
</body>
</html>
